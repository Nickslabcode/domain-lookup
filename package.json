{
  "name": "domain-info-lookup",
  "private": true,
  "version": "0.0.0-development",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "commit": "git cz",
    "acp": "git add . && npm run commit && git push origin dev",
    "dev:domainlookup": "npx wrangler dev --config workers/domainlookup/wrangler.toml",
    "deploy:domainlookup": "npx wrangler deploy --config workers/domainlookup/wrangler.toml",
    "semantic-release": "semantic-release --branches main"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "@types/dotenv": "^8.2.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/autoprefixer": "^10.2.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/tailwindcss": "^3.1.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^4.12.14",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "semantic-release": "^24.2.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "wrangler": "^3.86.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nickslabcode/domain-info-lookup.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
